Hodder.v <- function(y)#
{#
    n<-length(y)#
    for(i in 1:(n-1)) {#
        y[i] <- (y[i+1] - y[i])/211#
        y[1:(n-1)]#
#
    }#
    y <- c(0, y[1:(n-1)])#
#
