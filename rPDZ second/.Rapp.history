recognize_fold <- function(spectrum){#
    index <- which(Hodder.v(Hodder.v(spectrum$CPS))<(-0.002))#
    index[index %in% seq(41, 2040, 1)]#
}#
unfold_simple <- function(spectrum, index.seq){#
	    spectrum$CPSNew <- ifelse(as.numeric(rownames(spectrum)) %in% index.seq, spectrum$CPS+211, spectrum$CPS)#
#
    data.frame(Spectrum=spectrum$Spectrum, Energy=spectrum$Energy, CPS=spectrum$CPSNew)#
	}#
unfold_join <- function(spectrum){#
#
	index <-  recognize_fold(spectrum)#
	new.spec <- unfold_simple(spectrum=spectrum, index.seq=index)#
	result.list <- list(new.spec, recognize_fold(new.spec))#
	names(result.list) <- c("spectrum", "index")#
	result.list#
}#
#
unfold <- function(spectrum){#
#
    first_unfold <- unfold_join(spectrum)#
    second_unfold <- unfold_join(first_unfold)#
    third_unfold <- unfold_join(second_unfold)#
    fourth_unfold <- unfold_join(third_unfold)#
    fourth_unfold#
#
}
